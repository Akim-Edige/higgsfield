# Database
DB_DSN=postgresql+asyncpg://postgres:postgres@postgres:5432/app

# Redis
REDIS_URL=redis://redis:6379/0

# Higgsfield API
HIGGSFIELD_API_KEY=key
HIGGSFIELD_SECRET=secret
HIGGSFIELD_BASE=https://platform.higgsfield.ai

# Application
APP_DEBUG=false
POLL_MIN_INTERVAL_MS=1000
POLL_MAX_INTERVAL_MS=30000
POLL_JITTER=0.2
T2I_TIMEOUT_S=180
I2V_TIMEOUT_S=1200
T2V_TIMEOUT_S=1200

# === YANDEX CLOUD OBJECT STORAGE (публичный бакет) ===
S3_BUCKET=higgsfield
S3_REGION=ru-central1
S3_USE_PATH_STYLE=false
S3_ENDPOINT_INTERNAL=https://storage.yandexcloud.net
S3_PUBLIC_ENDPOINT=https://storage.yandexcloud.net
AWS_ACCESS_KEY_ID=key_id
AWS_SECRET_ACCESS_KEY=key

# Celery
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0
ANTHROPIC_API_KEY=key
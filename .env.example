# Database
DB_DSN=postgresql+asyncpg://postgres:postgres@postgres:5432/app

# Redis
REDIS_URL=redis://redis:6379/0

# Higgsfield API (REQUIRED - get from https://platform.higgsfield.ai)
HIGGSFIELD_API_KEY=your_api_key_here
HIGGSFIELD_SECRET=your_secret_here
HIGGSFIELD_BASE=https://platform.higgsfield.ai

# Application
APP_DEBUG=false
POLL_MIN_INTERVAL_MS=1000
POLL_MAX_INTERVAL_MS=30000
POLL_JITTER=0.2
T2I_TIMEOUT_S=180
I2V_TIMEOUT_S=1200
T2V_TIMEOUT_S=1200

# LocalStack S3 (dev)
S3_BUCKET=media
S3_REGION=us-east-1
S3_USE_PATH_STYLE=true
S3_ENDPOINT_INTERNAL=http://localstack:4566
S3_PUBLIC_ENDPOINT=http://localhost:4566
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test

# Celery
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0
